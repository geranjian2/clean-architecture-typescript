{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": 2022
    },
    "rules": {
        "trailing-comma": 0,
        "prefer-for-of": 0,
        "no-console": 0,
        "triple-equals": 0,
        "semi": 2
    },
    "parser": "babel-eslint"
}
